<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="OptionsDialog">
		<title>Options</title>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>1</cols>
			<growablecols>0</growablecols>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX3" variable="CheckBox_SendOffWhenNotRunning" member="yes">
					<label>Send data when not running sequence</label>
				</object>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX2" variable="CheckBox_Sync" member="yes">
					<label>Use ArtNet/E1.31 Synchronisation Protocols</label>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer7" member="no">
					<cols>2</cols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
							<label>Web Server Port:</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxSpinCtrl" name="ID_SPINCTRL1" variable="SpinCtrl_WebServerPort" member="yes">
							<value>80</value>
							<min>1</min>
							<max>64000</max>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
					<cols>3</cols>
					<growablecols>1</growablecols>
					<growablerows>0</growablerows>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Projectors:</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxGrid" name="ID_GRID1" variable="Grid_Projectors" member="yes">
							<cols>2</cols>
							<labelcolwidth>150</labelcolwidth>
							<defaultcolsize>200</defaultcolsize>
							<collabels>
								<item>IP Address</item>
								<item>Password</item>
							</collabels>
							<style>wxVSCROLL|wxHSCROLL</style>
							<handler function="OnGrid_ProjectorsCellLeftClick" entry="EVT_CMD_GRID_CELL_LEFT_CLICK" />
							<handler function="OnGrid_ProjectorsCellLeftDClick" entry="EVT_CMD_GRID_CELL_LEFT_DCLICK" />
							<handler function="OnGrid_ProjectorsLabelLeftClick" entry="EVT_CMD_GRID_LABEL_LEFT_CLICK" />
							<handler function="OnGrid_ProjectorsLabelLeftDClick" entry="EVT_CMD_GRID_LABEL_LEFT_DCLICK" />
							<handler function="OnGrid_ProjectorsLabelRightDClick" entry="EVT_CMD_GRID_LABEL_RIGHT_DCLICK" />
							<handler function="OnGrid_ProjectorsCellSelect" entry="EVT_CMD_GRID_SELECT_CELL" />
							<handler function="OnGrid_ProjectorsKeyDown" entry="EVT_KEY_DOWN" />
							<handler function="OnGrid_ProjectorsResize" entry="EVT_SIZE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="FlexGridSizer4" member="no">
							<cols>1</cols>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON4" variable="Button_AddProjector" member="yes">
									<label>Add</label>
									<handler function="OnButton_AddProjectorClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON8" variable="Button_ProjectorEdit" member="yes">
									<label>Edit</label>
									<handler function="OnButton_ProjectorEditClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON3" variable="Button_DeleteProjector" member="yes">
									<label>Delete</label>
									<handler function="OnButton_DeleteProjectorClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer5" member="no">
					<cols>3</cols>
					<growablecols>1</growablecols>
					<growablerows>0</growablerows>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>Buttons:    </label>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxGrid" name="ID_GRID2" variable="Grid_Buttons" member="yes">
							<cols>3</cols>
							<labelcolwidth>150</labelcolwidth>
							<defaultcolsize>150</defaultcolsize>
							<collabels>
								<item>Command</item>
								<item>Parameter</item>
								<item>Hotkey</item>
							</collabels>
							<size>800,200</size>
							<style>wxVSCROLL|wxHSCROLL</style>
							<handler function="OnGrid_ButtonsCellLeftClick" entry="EVT_CMD_GRID_CELL_LEFT_CLICK" />
							<handler function="OnGrid_ButtonsCellLeftDClick" entry="EVT_CMD_GRID_CELL_LEFT_DCLICK" />
							<handler function="OnGrid_ButtonsLabelLeftClick" entry="EVT_CMD_GRID_LABEL_LEFT_CLICK" />
							<handler function="OnGrid_ButtonsLabelLeftDClick" entry="EVT_CMD_GRID_LABEL_LEFT_DCLICK" />
							<handler function="OnGrid_ButtonsCellSelect" entry="EVT_CMD_GRID_SELECT_CELL" />
							<handler function="OnGrid_ButtonsKeyDown" entry="EVT_KEY_DOWN" />
							<handler function="OnGrid_ButtonsResize" entry="EVT_SIZE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="FlexGridSizer6" member="no">
							<cols>1</cols>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON5" variable="Button_ButtonAdd" member="yes">
									<label>Add</label>
									<handler function="OnButton_ButtonAddClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON6" variable="Button_ButtonEdit" member="yes">
									<label>Edit</label>
									<handler function="OnButton_ButtonEditClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON7" variable="Button_ButtonDelete" member="yes">
									<label>Delete</label>
									<handler function="OnButton_ButtonDeleteClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer8" member="no">
					<cols>2</cols>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
							<label>Web Directory:</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextCtrl_wwwRoot" member="yes">
							<value>xScheduleWeb</value>
							<handler function="OnTextCtrl_wwwRootText" entry="EVT_TEXT" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>3</cols>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="Button_Ok" member="yes">
							<label>Ok</label>
							<handler function="OnButton_OkClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON2" variable="Button_Cancel" member="yes">
							<label>Cancel</label>
							<handler function="OnButton_CancelClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
